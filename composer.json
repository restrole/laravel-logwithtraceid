{
  "name": "restrole/laravel-logwithtraceid",
  "description": "Adds trace_id to native Laravel logs and returns tid when frontend errors occur.",
  "keywords": [
    "log",
    "trace_id",
    "trace"
  ],
  "type": "library",
  "require": {
    "laravel/framework": "^10.0",
    "php": "^8.1"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Restrole\\LaravelLogWithTraceId\\": "src/"
    }
  },
  "authors": [
    {
      "name": "restrole",
      "email": "gary007.lang@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "extra": {
    "laravel": {
      "providers": [
        "Restrole\\LaravelLogWithTraceId\\ServiceProvider"
      ]
    }
  }
}
